;; -*- scheme -*-

(compile -s -O3 -d1
         -C "\"`pkg-config --cflags MagickWand`\""
         -L "\"`pkg-config --libs MagickWand`\""
         "magickwand.scm" -J)
(compile -s -O3 -d0 "magickwand.import.scm")

(install-extension
 'magickwand
 '("magickwand.so" "magickwand.import.so")
 '((version 0.1)))
